                            android:background="@color/line_football_footer" />

                        <com.hhly.mlottery.widget.NestedListView
                            android:id="@+id/basket_future_exploits_list_to"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:divider="#00000000"
                            android:dividerHeight="0dip"
                            android:listSelector="@android:color/transparent"></com.hhly.mlottery.widget.NestedListView>

                        <TextView
                            android:id="@+id/basket_analyze_nodata5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingBottom="@dimen/basket_details_item"
                            android:paddingTop="@dimen/basket_details_item"
                            android:text="@string/basket_nodata"
                            android:textColor="@color/mdy_666"
                            android:textSize="@dimen/text_size_16"
                            android:visibility="gone"/>
                    </LinearLayout>

                    <View
                        android:layout_width="1px"
                        android:layout_height="1px"
                        android:background="@color/black_details_textcolor" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/basketball_details_error"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingTop="200dp"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/refresh" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:text="@string/exp_net_status_txt"
                        android:textColor="@color/mdy_666"
                        android:textSize="14sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dip"
                            android:text="@string/exp__check_txt"
                            android:textColor="@color/mdy_666"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/basketball_details_error_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/exp_refresh_txt"
                            android:textColor="@color/basket_score"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/basket_analyze_details_nodata"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/basket_nodata"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="@color/mdy_666"
                    android:paddingTop="250dp"
                    android:visibility="gone"
                    />
            </RelativeLayout>
        </ScrollView>
    </com.hhly.mlottery.widget.ExactSwipeRefreshLayout>
</LinearLayout>
