<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--亚盘走势-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:text="@string/new_analyze_let"
            android:textColor="@color/analyze_title"
            android:textSize="@dimen/text_size_13"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:text="@string/new_analyze_guest"
            android:textSize="@dimen/text_size_10"
            android:textColor="@color/history_win"
            android:id="@+id/textView3" />
        <ImageView
            android:id="@+id/image_small_circular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/small_circular"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView3"
            android:layout_marginRight="5dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="@string/new_analyze_home"
            android:textSize="@dimen/text_size_10"
            android:textColor="@color/history_win"
            android:layout_toLeftOf="@id/image_small_circular"
            android:id="@+id/textView4" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/small_triangle"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/textView4"
            android:layout_marginRight="5dp"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/line_football_footer"
        />

    <LinearLayout
        android:id="@+id/ll_analyze_let"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingRight="10dp"
        android:paddingLeft="10dp"
        >
        <TextView
            android:id="@+id/tv_analyze_let1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:text=""
            android:textSize="@dimen/text_size_13"
            />
        <TextView
            android:id="@+id/tv_analyze_let2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text=""
            android:textSize="@dimen/text_size_13"
            android:layout_marginBottom="10dp"
            />

        <LinearLayout
            android:id="@+id/ll_analyze_let_history"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <!--赢 走 输-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:orientation="vertical"
                    style="@style/analyze_ll_let_win_lose"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_win"
                        android:gravity="top"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/football_analyze_win_color"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_draw"
                        android:gravity="center"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/more_record"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_lose"
                        android:gravity="bottom"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/football_analyze_lose_color"
                        />
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >
                    <com.hhly.mlottery.widget.LineChartView
                        android:id="@+id/chart_let_history"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                    <TextView
                        android:id="@+id/let_nodata1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/basket_nodata"
                        android:layout_centerInParent="true"
                        android:textColor="@color/black_details_ball_textcolor"
                        android:visibility="gone"
                        />
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/line_football_footer"/>
            <!--比分 初盘 结果-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:orientation="vertical"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    style="@style/analyze_ll_let_win_lose"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_score"
                        android:gravity="center"
                        style="@style/analyze_text_score"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_first_let"
                        android:gravity="center"
                        style="@style/analyze_text_score"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/new_analyze_result"
                        android:gravity="center"
                        style="@style/analyze_text_score"
                        />
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="10dp"
                    >
                    <com.hhly.mlottery.widget.NoScrollListView
                        android:id="@+id/lv_analyze_let"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:listSelector="#00000000"/>
                    <TextView
                        android:id="@+id/let_nodata2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/basket_nodata"
                        android:layout_centerInParent="true"
                        android:textColor="@color/black_details_ball_textcolor"
                        android:visibility="gone"
                        />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
        <!--近期对比-->
