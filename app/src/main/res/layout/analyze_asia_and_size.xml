                        android:gravity="top"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/football_analyze_lose_color"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="走"
                        android:gravity="center"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/more_record"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="大"
                        android:gravity="bottom"
                        android:textSize="@dimen/text_size_13"
                        android:textColor="@color/football_analyze_win_color"
                        />
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/size_guest_team"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_10"
                        android:textColor="@color/black_details_ball_textcolor"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        />
                    <com.hhly.mlottery.widget.LineChartView
                        android:id="@+id/chart_size_guest"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                    <TextView
                        android:id="@+id/size_nodata4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/basket_nodata"
                        android:layout_centerInParent="true"
                        android:textColor="@color/black_details_ball_textcolor"
                        android:visibility="gone"
                        />
                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>
        <RadioGroup
            android:id="@+id/radio_group_size"
            android:layout_width="260dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal">

            <!--历史交锋-->
            <RadioButton
                android:id="@+id/size_rb_history"
                style="@style/analyze_radio_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/analyze_selected_left"
                android:checked="true"
                android:text="@string/basket_analyze_history"/>

            <!--近期对比-->
            <RadioButton
                android:id="@+id/size_rb_recent"
                style="@style/analyze_radio_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/analyze_selected_right"
                android:text="@string/new_analyze_recent_compare"/>

        </RadioGroup>
        <TextView
            android:id="@+id/tv_analyze_more_message2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingBottom="15dp"
            android:text="@string/new_analyze_more_messages"
            android:textColor="@color/more_record"
            android:textSize="@dimen/text_size_13"/>
    </LinearLayout>
    <TextView
        android:id="@+id/size_all_nodata"
        android:layout_width="match_parent"
        android:layout_height="@dimen/basket_analyze_nodata_hight"
        android:paddingBottom="@dimen/basket_details_item_height"
        android:paddingTop="@dimen/basket_details_item_height"
        android:textSize="@dimen/text_size_13"
        android:text="@string/basket_nodata"
        android:textColor="@color/black_details_ball_textcolor"
        android:gravity="center"
        android:visibility="gone"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/home_item_bg"/>
</LinearLayout>