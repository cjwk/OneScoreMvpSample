        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:gravity="center"
            android:text="@string/set_goal_txt"
            android:textColor="@color/res_time_color"
            android:textSize="@dimen/item_football_half_score" />

        <TextView
            android:id="@+id/tv_data_goal_total"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/res_time_color"
            android:textSize="@dimen/item_football_half_score"
            tools:text="1" />

        <TextView
            android:id="@+id/tv_data_goal_zu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/res_time_color"
            android:textSize="@dimen/item_football_half_score"
            tools:text="1" />

        <TextView
            android:id="@+id/tv_data_goal_ke"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/res_time_color"
            android:textSize="@dimen/item_football_half_score"
